{
  "name": "@metastable/cppzst",
  "version": "2.1.0",
  "description": "Zstd wrapper for Nodejs 14 and above",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "require": "./esm/index.js",
      "import": "./cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "npm run build:cjs && npm run build:esm && npm run compile",
    "build:esm": "tsc --module esnext --outDir esm && echo '{\"type\": \"module\"}' > esm/package.json",
    "build:cjs": "tsc --module commonjs --outDir cjs && echo '{\"type\": \"commonjs\"}' > cjs/package.json",
    "clean": "node-gyp clean",
    "install": "node-gyp-build",
    "compile": "node-gyp rebuild",
    "prebuild": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mat-sz/cppzst.git"
  },
  "keywords": [
    "zstd",
    "compression",
    "decompression"
  ],
  "author": "zwb-ict",
  "contributors": [
    {
      "name": "gcxfd",
      "email": "i@rmw.link",
      "url": "https://github.com/gcxfd"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.22.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "node-gyp": "^11.0.0",
    "prebuildify": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "binary": {
    "napi_versions": [
      9
    ]
  }
}
