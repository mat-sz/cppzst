    }
  ],
  "license": "MIT",
  "dependencies": {
    "nan": "^2.19.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "node-gyp": "^10.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
